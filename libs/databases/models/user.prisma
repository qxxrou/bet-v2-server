model user {
  id Int @id @unique @default(autoincrement())
  up_id Int 
  reg_time DateTime @default(now()) 
  log_time DateTime @updatedAt
  user_name String @db.VarChar(64) @unique
  password String @db.VarChar(64)
  phone String @db.VarChar(64) @unique
  email String @db.VarChar(64) @unique

  real_name String @unique
}