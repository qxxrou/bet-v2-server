{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/base",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "apps/base/tsconfig.app.json",
    "webpack": true
  },
  "generateOptions": {
    "spec": false
  },
  "monorepo": true,
  "root": "apps/base",
  "projects": {
    "base": {
      "type": "application",
      "root": "apps/base",
      "entryFile": "main",
      "sourceRoot": "apps/base/src",
      "compilerOptions": {
        "tsConfigPath": "apps/base/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "wallet": {
      "type": "application",
      "root": "apps/wallet",
      "entryFile": "main",
      "sourceRoot": "apps/wallet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wallet/tsconfig.app.json"
      }
    }
  }
}